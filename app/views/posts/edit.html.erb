<h1>Update <%= @post.title %></h1>

<%= form_for @post do |f| %>
<%= f.label :title %>
<%= f.text_field :title %>
<%= f.label :content %>
<%= f.text_area :content %>
<%= f.label :blogger %>
<%= f.collection_select(:blogger_id, Blogger.all, :id, :name, prompt: true)%>
<%= f.label :destination %>
<%= f.collection_select(:destination_id, Destination.all, :id, :name, prompt: true)%>
<%= f.submit %>
<% end %>